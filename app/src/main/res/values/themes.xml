<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="SIDWebTheme" parent="Theme.AppCompat.Light">
        <item name="actionBarStyle">@style/MyTheme.ActionBarStyle</item>
        <item name="actionBarItemBackground">@color/default_actionbar</item>
    </style>
    <style name="MyTheme.ActionBarStyle" parent="Widget.AppCompat.Light.ActionBar">
        <item name="titleTextStyle">@style/MyTheme.ActionBar.TitleTextStyle</item>
        <item name="background">@color/default_actionbar</item>
        <item name="windowActionBar">true</item>
    </style>
    <style name="MyTheme.ActionBar.TitleTextStyle" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@android:color/white</item>
    </style>

</resources>